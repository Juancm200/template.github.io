{
  "name": "matt-fantinel-website",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host",
    "build": "rm -rf build && vite build && touch build/.nojekyll && mv build/404/index.html build/404.html",
    "deploy": "npm run build && npx gh-pages -d build -t true",
    "preview": "vite preview",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "postbuild": "svelte-sitemap --domain https://fantinel.dev --trailing-slashes true"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.38",
    "@sveltejs/kit": "next",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^4.0.0",
    "install": "^0.13.0",
    "mdsvex": "^0.10.6",
    "npm": "^8.16.0",
    "prettier": "~2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.0.0",
    "rehype-slug": "^5.0.1",
    "sass": "^1.54.3",
    "svelte": "^3.49.0",
    "svelte-preprocess": "^4.10.7",
    "svelte-sitemap": "^2.5.1",
    "vite": "^3.0.4"
  },
  "type": "module",
  "dependencies": {
    "dateformat": "^5.0.3",
    "editor.md": "^1.5.0",
    "firebase": "^9.9.2",
    "firebase-tools": "^11.4.2"
  }
}
